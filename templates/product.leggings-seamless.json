        "1663667029f86d6497-1": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Can I purchase items with another currency?",
            "open": false,
            "content": "<p>Yes, you may select a currency based on your personal preference. When you select your country in the country selector on the upper right of the website or are taken directly to your country‚Äôs version of the website, you will see prices listed in the regional currency.<\/p>",
            "page": ""
